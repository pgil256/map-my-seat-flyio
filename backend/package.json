{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest -i",
    "test:coverage": "jest -i --coverage"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "config.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/migrations/**",
      "!**/coverage/**",
      "!knexfile.js",
      "!server.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "formidable": "^3.5.0",
    "fs": "^0.0.1-security",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "objection": "^3.1.1",
    "pg": "^8.11.2",
    "postgres": "^3.3.5",
    "psql": "^0.0.1",
    "serve-favicon": "^2.5.0",
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.8",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  }
}
